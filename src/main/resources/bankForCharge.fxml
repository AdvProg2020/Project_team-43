<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="900.0"
            style="-fx-background-image: url('newbank.jpg'); -fx-background-repeat: stretch; -fx-background-position: center center; -fx-background-size: 900 600 "
            xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="View.GraphicController.BankForChargeController">
    <children>
        <AnchorPane prefHeight="433.0" prefWidth="601.0">
            <children>
                <JFXTextField fx:id="bankUsername" layoutX="177.0" layoutY="68.0"/>
                <Label layoutX="43.0" layoutY="83.0" text="bank username"/>
                <JFXTextField fx:id="bankPassword" layoutX="177.0" layoutY="123.0"/>
                <Label layoutX="43.0" layoutY="144.0" text="bank password"/>
                <JFXTextField fx:id="amount" layoutX="177.0" layoutY="186.0"/>
                <Label layoutX="57.0" layoutY="204.0" text="amount"/>
                <JFXTextField fx:id="accountId" layoutX="177.0" layoutY="25.0"/>
                <Label layoutX="43.0" layoutY="41.0" text="account ID"/>
                <ImageView fitHeight="44.0" fitWidth="139.0" layoutX="108.0" layoutY="278.0" onMouseClicked="#pay"
                           pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@pay_button.png"/>
                    </image>
                </ImageView>
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
